<!DOCTYPE html>
<html>
  <%- include("./partials/head") %>
  <body>
    <div class="container-fluid">
      <div class="row">
        <div class="col-sm-2">
          <%- include("./partials/sidebar.ejs") %>
        </div>
        <div class="col-sm-10">
          <h1><%= title %></h1>
          <ul>
            <% for (let i = 0; i < dairy_list.length; i++) { %>
            <li>
              <a href="<%= dairy_list[i].url %>"
                ><%= dairy_list[i].name %> (<%= dairy_list[i].price
                %>)</a
              >
            </li>
            <% } %> <% if (dairy_list.length === 0) { %>
            <li>There is no dairy.</li>
            <% } %>
          </ul>
        </div>
      </div>
    </div>
  </body>
</html>
